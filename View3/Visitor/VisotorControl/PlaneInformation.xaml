<UserControl x:Class="View3.Visitor.VisotorControl.PlaneInformation"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:View3.Visitor.VisotorControl"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Loaded="loaded">
    <Grid Background="White">
        <DataGrid Grid.Column="1" x:Name="myDataGrid" HorizontalAlignment="Stretch" 
                  Margin="10,10,10,10" VerticalAlignment="Stretch"
                  SelectionUnit="FullRow" IsReadOnly="True"/>
    </Grid>
</UserControl>
